# Server Configuration
PORT=3001
SERVER_NAME=Roblex Studio MCP Server
SERVER_VERSION=1.0.0
REQUIRE_AUTH=false
NODE_ENV=development

# Authentication
MCP_API_KEY_ROBLEX_STUDIO=your_api_key_here
